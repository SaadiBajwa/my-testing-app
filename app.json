{
  "expo": {
    "name": "my-testing-app",
    "slug": "my-testing-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconapp.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/chat.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.saadali25.mytestingapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
